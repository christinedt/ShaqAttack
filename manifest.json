{
"manifest_version": 2,
    "name": "Shaq Attack",
    "short_name": "ShaqAttack",
    "description": "Shaq deserves his own week way more than sharks do, so this extension replaces shark mentions with awesome Shaq mentions.",
    "version": "0.4",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" 
    }
}